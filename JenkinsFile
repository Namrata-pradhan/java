pipeline {
    agent any

    triggers {
        pollSCM '* * * * *'
    }
     stages {
        stage('Build') {
            steps {
                echo 'building'
                sh 'java --version'
            }
        }
        stage('Test') {
            steps {
                echo 'testing'
            }
        }
        stage('Deliver') {
            steps {
                echo 'delivery'
            }
        }
    }
}
